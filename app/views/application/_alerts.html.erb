<% if flash.to_hash.length > 0 %>
  <!-- <button class="close" data-dismiss="alert"><span class="glyphicon glyphicon-remove" style="margin-top:14px; margin-right: 14px"></span></button> -->
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value.html_safe %></div>
  <% end %>
<% end %>
